{
    "name": "onesite/bidv-openapi",
    "description": "Connect to BIDV OpenAPI.",
    "license": "MIT",
    "authors": [{
        "name": "https://www.tungzero.xyz",
        "email": "tungnt.blue@gmail.com",
        "role": "Technical Manager"
    }],
    "require": {
        "php": "^7.1 || ^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "web-token/jwt-framework": "^2.2",
        "nesbot/carbon": "^3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.5 || ^9.3",
        "dflydev/dot-access-data": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v1.0.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "OneSite\\BIDV\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "OneSite\\BIDV\\Tests\\": "tests"
        }
    }
}